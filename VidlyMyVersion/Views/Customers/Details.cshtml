@model VidlyMyVersion.Models.Customer
@{
    ViewBag.Title = "Details";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>@Model.Name</h2>

<ul>
    <li>Memberhip Type: @Model.MembershipType.Name</li>
    @if(Model.Birthday!=null)
     {
          <li>Birthdate:@Model.Birthday.Value.ToLongDateString()</li>
     }
</ul>

